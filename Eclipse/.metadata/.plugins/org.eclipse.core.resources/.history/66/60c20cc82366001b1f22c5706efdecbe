package com;

public class Employee {
	private int id;
	private String name;
	private float salary;			// instance variable
	
	Employee() {
	System.out.println("object created");
	}
	
	void display() {
		System.out.println("id is "+id);
		System.out.println("name is "+name);
		System.out.println("salary is "+salary);
	}
}

