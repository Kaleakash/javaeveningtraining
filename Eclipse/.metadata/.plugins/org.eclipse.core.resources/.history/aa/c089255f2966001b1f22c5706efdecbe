package com;
class Abc {
	int a;
	static int b;
	void dis1() {
		System.out.println("Non static method");
	}
	static void dis2() {
		System.out.println("Static method");
	}
}
public class DemoTest {
	public static void main(String[] args) {
		Abc obj1 = new Abc();
		obj1.a=100;
		Abc.b=200;		//static variable;
		obj1.dis1();
		Abc.dis2();    //static method
		obj1.b=30;
		obj1.dis2();
	}

}
