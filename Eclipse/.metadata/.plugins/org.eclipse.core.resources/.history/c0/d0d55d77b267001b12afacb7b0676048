package com;
class AA implements Runnable{
		@Override
		public void run() {
			
		}
}
class BB implements Runnable{
	@Override
	public void run() {
			
	}
}
public class ImplementsRunnable {
	public static void main(String[] args) {
	Runnable obj1 = new AA();
	Runnable obj2 = new BB();
	Thread t1 = new Thread(obj1);			
	}

}
