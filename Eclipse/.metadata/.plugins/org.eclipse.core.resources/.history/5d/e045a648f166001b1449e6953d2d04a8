package com;

public class ThrowsDemo {
	static void dis1() {
		int a=10/0;
		System.out.println("this is dis1() method");
	}
	static void dis2() {
			dis1();
			System.out.println("this is dis2() method");
	}
	public static void main(String[] args) {
		dis2();
		System.out.println("this is main method");
	}

}
