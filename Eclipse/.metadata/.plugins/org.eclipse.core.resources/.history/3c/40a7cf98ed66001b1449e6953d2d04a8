package com;

public class TryAndCatchBlock {

	public static void main(String[] args) {
		System.out.println("Hi");
		int a=10;
		int b=0;
		try {
		int res = a/b;			// 10/0
		System.out.println("Result is "+res);
		}catch (Exception e) {
		System.out.println("I Take Care!");
		}
		System.out.println("bye");
		System.out.println("bye");

	}

}
