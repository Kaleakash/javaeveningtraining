package com;

public class TryAndCatchBlock {

	public static void main(String[] args) {
		System.out.println("Hi");
		int a=10;
		int b=1;
		int abc[]= {10,20,30,40};
		try {
		int res = a/b;			// 10/0
		System.out.println("Result is "+res);
		int res1 = 10/ab[6];
		}catch (Exception e) {
		//System.out.println("I Take Care!");
			System.out.println(e.toString());
		}
		System.out.println("bye");
		System.out.println("bye");

	}

}
