package com;
class A1 implements Runnable{
	@Override
	public void run() {
		Thread t1 = Thread.currentThread();
		for(int i=0;i<=10;i++) {
		System.out.println(t1+" "+i);
		}
	}
}
class B1 implements Runnable{
@Override
public void run() {
	Thread t1 = Thread.currentThread();
	for(int j=0;j<=10;j++) {
		System.out.println(t1+" "+j);
	}
}
}
public class LifeCycleMethods {
	public static void main(String[] args) {
			A1 obj1 = new A1();
			B2 obj2 = new B1();
	}

}
