package com;
class Abc implements Runnable {
	@Override
	public void run() {
	String name = Thread.currentThread().getName();
	for(int i=0;i<10;i++) {
		try {
			System.out.println(name+" "+i);
			Thread.sleep(1000);
		} catch (Exception e) {
			// TODO: handle exception
		}
	}
	}
}
public class WaitAndNotifyAll {

	public static void main(String[] args) {
	

	}

}
